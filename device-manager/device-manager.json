  "paths" : {
    "/drone-models" : {
      "get" : {
        "tags" : [ "device manager" ],
        "summary" : "returns list of supported drone models",
        "description" : "gets the supported drone models",
        "operationId" : "getDroneModels",
        "responses" : {
          "200" : {
            "description" : "list of supported drone models",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "array",
                  "items" : {
                    "$ref" : "#/components/schemas/DroneModel"
